<div class="well col-xs-8 col-sm-8 col-md-8 col-lg-8 col-xs-offset-2 col-sm-offset-2 col-md-offset-2 col-lg-offset-2">
    {{#each data}}
    <div class="row user-row">
        <div class="member-img-small col-xs-3 col-sm-2 col-md-1 col-lg-1">
            {{this.profileImage}}
        </div>
        <div class="col-xs-8 col-sm-9 col-md-10 col-lg-10">
            <strong>{{this.username}}</strong><br>
            <span class="text-muted">User level: Member</span>
        </div>
        <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1 dropdown-user" data-for=".cyruxx">
            <i class="glyphicon glyphicon-chevron-down text-muted"></i>
        </div>
    </div>
    <div class="row user-infos cyruxx">
        <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10 col-xs-offset-0 col-sm-offset-0 col-md-offset-1 col-lg-offset-1">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">User information</h3>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="member-img-big col-md-3 col-lg-3 hidden-xs hidden-sm">
                            {{this.profileImage}}
                        </div>
                        <div class="member-img-big col-xs-2 col-sm-2 hidden-md hidden-lg">
                            {{this.profileImage}}
                        </div>
                        <div class="col-xs-10 col-sm-10 hidden-md hidden-lg">
                            <strong>{{this.username}}</strong><br>
                            <dl>
                                <dt>User level:</dt>
                                <dd>Member</dd>
                                <dt>Email:</dt>
                                <dd>{{this.email}}</dd>
                            </dl>
                        </div>
                        <div class=" col-md-9 col-lg-9 hidden-xs hidden-sm">
                            <strong>{{this.username}}</strong><br>
                            <table class="table table-user-information">
                                <tbody>
                                    <tr>
                                        <td>User level:</td>
                                        <td>Member</td>
                                    </tr>
                                    <tr>
                                        <td>Email:</td>
                                        <td>{{this.email}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="panel-footer">
                    <a href="#/chat" class="btn btn-sm btn-primary" type="button" data-toggle="tooltip" data-original-title="Send message to user"><i class="glyphicon glyphicon-envelope"></i></button>
                    <span class="pull-right">
                        </span>
                </div>
            </div>
        </div>
    </div>
    {{/each}}
</div>